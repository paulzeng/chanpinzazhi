package com.chanpinzazhi.entity;

public class RelevantData { 
	private int RelevantNavigateId;
	private int RelevantDataId;
	private String RelevantDataName;
	public RelevantData() { 
	}
	
	public RelevantData(int relevantNavigateId,int relevantDataId, String relevantDataName) {
		super();
		RelevantNavigateId = relevantNavigateId;
		RelevantDataId = relevantDataId;
		RelevantDataName = relevantDataName;
	}
	
	public int getRelevantNavigateId() {
		return RelevantNavigateId;
	}

	public void setRelevantNavigateId(int relevantNavigateId) {
		RelevantNavigateId = relevantNavigateId;
	}

	public int getRelevantDataId() {
		return RelevantDataId;
	}
	public void setRelevantDataId(int relevantDataId) {
		RelevantDataId = relevantDataId;
	}
	public String getRelevantDataName() {
		return RelevantDataName;
	}
	public void setRelevantDataName(String relevantDataName) {
		RelevantDataName = relevantDataName;
	}
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return RelevantDataId+","+RelevantDataName;
	}
}
