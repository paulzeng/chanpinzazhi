package com.chanpinzazhi.entity;
/**
 * @author Administrator
 *
 */
public class Category {
	private int mCategoryId;
	private String mName;
	private String mDesc;
	private String mImgUrl;
	private String mServerTime;
	public Category(int mCategoryId,String mName,String mDesc,String mImgUrl,String mServerTime){
		this.mCategoryId = mCategoryId;
		this.mName = mName;
		this.mDesc = mDesc;
		this.mImgUrl = mImgUrl;
		this.mServerTime = mServerTime;
	}
	public Category(){}
	public int getmCategoryId() {
		return mCategoryId;
	}
	public void setmCategoryId(int mCategoryId) {
		this.mCategoryId = mCategoryId;
	}
	public String getmName() {
		return mName;
	}
	public void setmName(String mName) {
		this.mName = mName;
	}
	public String getmDesc() {
		return mDesc;
	}
	public void setmDesc(String mDesc) {
		this.mDesc = mDesc;
	}
	public String getmImgUrl() {
		return mImgUrl;
	}
	public void setmImgUrl(String mImgUrl) {
		this.mImgUrl = mImgUrl;
	}
	public String getmServerTime() {
		return mServerTime;
	}
	public void setmServerTime(String mServerTime) {
		this.mServerTime = mServerTime;
	}
	@Override
	public String toString() { 
		return "mCategoryId"+mCategoryId+"|"+"mName"+mName+"|"+"mDesc"+mDesc+"|"+"mImgUrl"+mImgUrl+"|"+"mServerTime"+mServerTime;
	}
}
